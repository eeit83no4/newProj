<!DOCTYPE validators PUBLIC
        "-//Apache Struts//XWork Validator 1.0.2//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
    <field name="store">
        <field-validator type="requiredstring">
        	 <param name="trim">true</param>
            <message key="store_error1"></message>           
        </field-validator>
    </field>
    <field name="storeClass">
        <field-validator type="requiredstring">
       		  <param name="trim">true</param>
            <message key="password_error55"/>
        </field-validator>
    </field>
    <field name="attributes">
        <field-validator type="requiredstring">
       		  <param name="trim">true</param>
            <message key="password_error55"/>
        </field-validator>
    </field>
    <field name="item">
        <field-validator type="requiredstring">
       		  <param name="trim">true</param>
            <message key="password_error55"/>
        </field-validator>
    </field>
    <field name="phone">
        <field-validator type="requiredstring">
       		  <param name="trim">true</param>
            <message key="password_error55"/>
        </field-validator>
    </field>  
     <field name="address">
        <field-validator type="requiredstring">
       		  <param name="trim">true</param>
            <message key="password_error55"/>
        </field-validator>
    </field>       
    <field name="submit">
        <field-validator type="requiredstring">
       		  <param name="trim">true</param>
            <message key="password_error55"/>
        </field-validator>
    </field>  
</validators>
